{
  "name": "zazu-template",
  "version": "1.0.0",
  "description": "Zazu Template for starting your own plugin.",
  "blocks": {
    "input": [
      {
        "id": "user-input",
        "type": "Keyword",
        "keyword": "usernames",
        "title": "Generate Usernames",
        "subtitle": "Generate a list of usernames",
        "icon": "fa-user-tag",
        "connections": ["user"]
      },
      {
        "id": "user",
        "type": "PrefixScript",
        "prefix": "â˜ƒ",
        "space": false,
        "args": "Optional",
        "script": "src/usernames.js",
        "debounce": 75,
        "connections": ["Copy"]
      }
    ],
    "output": [
      {
        "id": "Copy",
        "type": "CopyToClipboard",
        "text": "{value}"
      }
    ]
  }
}

{
  "name": "zazu-template",
  "version": "1.0.0",
  "description": "Zazu Template for starting your own plugin.",
  "blocks": {
    "input": [
      {
        "id": "user",
        "type": "PrefixScript",
        "prefix": "user",
        "space": false,
        "args": "Optional",
        "script": "src/usernames.js",
        "debounce": 75,
        "connections": ["Copy"]
      }
    ],
    "output": [
      {
        "id": "Copy",
        "type": "CopyToClipboard",
        "text": "{value}"
      }
    ]
  }
}
